{
  "schemaVersion": 1,
  "id": "blockshifter",
  "version": "${version}",
  "name": "Blockshifter",
  "description": "This mod lets you slide blocks on special rails.",
  "authors": [
    "LolHens"
  ],
  "contact": {
    "homepage": "https://github.com/LolHens/mc-blockshifter",
    "sources": "https://github.com/LolHens/mc-blockshifter"
  },
  "license": "Apache-2.0",
  "icon": "assets/blockshifter/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "scala",
        "value": "de.lolhens.minecraft.blockshifter.BlockshifterMod"
      }
    ]
  },
  "mixins": [
    "blockshifter.mixins.json"
  ],
  "depends": {
    "fabric-language-scala": "*",
    "fabricloader": ">=0.7.4",
    "fabric": ">=0.43.1",
    "minecraft": "1.18.x"
  }
}
